string:split
	a rpl:Function ;
	rpl:arity 2 ;
	rpl:parameters (
		[
			rdfs:label "s" ;
			rpl:parameterType xsd:string ;
			rpl:isTransparent true ]
		[
			rdfs:label "regex" ;
			rpl:parameterType xsd:string ;
			rpl:isTransparent true ]
