			rpl:parameterType rdf:List ;
			rpl:isTransparent true ]
		) ;
	rdfs:label "of" ;
	rdfs:comment "i l  =>  l[i]  -- pushes the member of List l at index i.  Note: lists are 1-indexed" ;
